/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  font-size: 21px;
} 
image{
  width: 100%;
  height: 100%;
}
.container{
  font-size: 21.4px;
}
page{
  width: 100%;
  height: 100%;
  font-size: 24rpx;
}
@media screen and (max-width: 300px) {
  .page{
    font-size: 41.4px;
  }      
}


